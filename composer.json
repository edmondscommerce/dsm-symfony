{
    "name": "edmondscommerce/dsm-symfony",
    "description": "A symfony bundle for Doctrine Static Meta (edmondscommerce/doctrine-static-meta) and Symfony",
    "type": "library",
    "require": {
        "edmondscommerce/doctrine-static-meta": "dev-master@dev"
    },
    "require-dev": {
        "edmondscommerce/phpqa": "dev-master@dev"
    },
    "authors": [
        {
            "name": "Edmonds Commerce",
            "email": "info@edmondscommerce.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "EdmondsCommerce\\DsmApiPlatformBundle\\": ["src/"]
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
