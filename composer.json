{
    "name": "edmondscommerce/dsm-symfony",
    "description": "A symfony bundle for Doctrine Static Meta (edmondscommerce/doctrine-static-meta) and Symfony",
    "type": "library",
    "require": {
        "edmondscommerce/doctrine-static-meta": "^2.3.6",
        "dittto/doctrine-entity-factories": "^2.0.0"
    },
    "require-dev": {
        "edmondscommerce/phpqa": "dev-master@dev"
    },
    "authors": [
        {
            "name": "Edmonds Commerce",
            "email": "info@edmondscommerce.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "EdmondsCommerce\\DsmBundle\\": [
                "DsmBundle/"
            ]
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:rossmitchell/doctrine-entity-factories.git"
        }
    ]
}
